{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "kebab-case"
    ],
    "use-host-property-decorator": false,
    "variable-name": false,
    "no-host-metadata-property": false,
    "no-inferrable-types": false,
    "no-input-rename": false,
    "directive-class-suffix": false,
    "component-class-suffix": false,
    "curly": false,
    "no-bitwise": false
  }
}
